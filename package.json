{
  "name": "buddy",
  "version": "0.0.1-alpha.1",
  "description": "Dead simple cross-domain iframe messaging",
  "main": "dist/buddy.cjs.js",
  "repository": "https://github.com/p3ol/buddy.git",
  "author": "Ugo Stephant <ugo@poool.fr>",
  "license": "MIT",
  "private": true,
  "sideEffects": false,
  "jsnext:main": "dist/lib/index.js",
  "module": "dist/lib/index.js",
  "unpkg": "dist/buddy.min.js",
  "cdn": "dist/buddy.min.js",
  "esnext": "src",
  "scripts": {
    "build:module": "BABEL_ENV=module ./node_modules/.bin/babel ./src --out-dir ./dist/lib",
    "build:browser": "BABEL_ENV=browser ./node_modules/.bin/rollup -c",
    "build": "yarn build:browser && yarn build:module",
    "prepack": "yarn build && yarn test",
    "pretest": "yarn build",
    "test": "BABEL_ENV=tests ./node_modules/.bin/karma start karma.config.js",
    "changelog": "./node_modules/.bin/auto-changelog",
    "serve": "./node_modules/.bin/webpack-dev-server --config ./webpack.config.js --hot --inline --open"
  },
  "browserslist": [
    "last 2 versions",
    "Safari >= 10",
    "iOS >= 10",
    "not ie <= 10",
    "> 1%"
  ],
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "auto-changelog": "^1.16.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "eslint": "^6.6.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jasmine": "^2.10.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jasmine": "^3.5.0",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^1.2.0",
    "karma-jasmine": "^2.0.1",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-rollup-preprocessor": "^7.0.2",
    "rollup": "^1.26.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "core-js": "^3.3.5"
  }
}
