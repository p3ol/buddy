    "test": "BABEL_ENV='tests' ./node_modules/.bin/karma start karma.config.js",
    "prepack": "yarn lint && yarn build && yarn test",
    "changelog": "./node_modules/.bin/auto-changelog",
    "serve": "./node_modules/.bin/webpack-dev-server --config ./webpack.config.js --hot --inline --open",
