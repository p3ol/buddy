{
  "compilerOptions": {
    "baseUrl": ".",
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "target": "ES5",
    "resolveJsonModule": true,
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true
  }
}
